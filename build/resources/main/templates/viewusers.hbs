{{#partial "content"}}
<h1>All Employees</h1>
<h2>To add a user:
    <a href="/userform"> click here</a>
</h2>
<br>
<br>
<table class="table">
    <tr>
        <th scope="col">Employee Name</th>
        <th scope="col">Department Id</th>
        <th scope="col">Role</th>
        <th scope="col">Position</th>


    </tr>
    {{#each users}}
        <tr>
            <td>{{username}}</td>
            <td>{{idDepartment}}</td>
            <td>{{role}}</td>
            <td>{{position}}</td>

        </tr>
    {{/each}}
</table>

{{/partial}}

{{>layout.hbs}}
